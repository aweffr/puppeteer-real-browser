{
  "name": "puppeteer-real-browser-for-llm",
  "version": "0.0.1",
  "description": "A fork of puppeteer-real-browser, optimized for LLM applications. Original project: https://github.com/zfcsoftware/puppeteer-real-browser",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.mjs",
  "exports": {
    ".": {
      "require": "./lib/cjs/index.js",
      "import": "./lib/esm/index.mjs",
      "types": "./typings.d.ts"
    }
  },
  "typings": "typings.d.ts",
  "scripts": {
    "esm_test": "node ./test/esm/test.js",
    "cjs_test": "node ./test/cjs/test.js"
  },
  "keywords": [
    "puppeteer-real-browser-fork",
    "llm-automation",
    "cf-bypass",
    "cloudflare-bypass",
    "puppeteer-fingerprint",
    "puppeteer-cloudflare"
  ],
  "author": "aweffr",
  "license": "MIT",
  "dependencies": {
    "chrome-launcher": "^1.1.2",
    "ghost-cursor": "^1.3.0",
    "puppeteer-extra": "^3.3.6",
    "rebrowser-puppeteer-core": "^23.3.1",
    "tree-kill": "^1.2.2"
  },
  "devDependencies": {
    "xvfb": "^0.4.0"
  },
  "peerDependencies": {
    "xvfb": "*"
  },
  "peerDependenciesMeta": {
    "xvfb": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aweffr/puppeteer-real-browser"
  },
  "readme": "README.md",
  "homepage": "https://github.com/aweffr/puppeteer-real-browser"
}
